{
  "name": "@b0ase/handcash",
  "version": "0.1.0",
  "description": "Unified HandCash integration for all b0ase apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./provider": {
      "types": "./dist/provider.d.ts",
      "import": "./dist/provider.mjs",
      "require": "./dist/provider.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/server.ts src/client.ts src/provider.tsx --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/server.ts src/client.ts src/provider.tsx --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@handcash/handcash-connect": "^0.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "handcash",
    "bitcoin",
    "bsv",
    "wallet",
    "payments",
    "b0ase"
  ],
  "author": "b0ase",
  "license": "MIT"
}
