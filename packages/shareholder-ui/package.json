{
  "name": "@b0ase/shareholder-ui",
  "version": "0.1.0",
  "description": "Cap table and shareholder display types and utilities",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["shareholder", "cap-table", "equity", "tokens", "holders", "distribution"],
  "license": "MIT"
}
