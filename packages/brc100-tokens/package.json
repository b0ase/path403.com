{
  "name": "@b0ase/brc100-tokens",
  "version": "0.1.0",
  "description": "BRC-100 token standard implementation for programmable tokens on Bitcoin",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["brc-100", "bitcoin", "tokens", "ordinals", "programmable", "defi"],
  "license": "MIT"
}
