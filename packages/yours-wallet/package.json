{
  "name": "@b0ase/yours-wallet",
  "version": "0.1.0",
  "description": "Yours Wallet integration for BSV payments, auth, and ordinals",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["yours", "wallet", "bsv", "ordinals", "1sat", "payments"],
  "license": "MIT"
}
