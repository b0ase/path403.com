{
  "name": "@b0ase/unified-auth-ui",
  "version": "0.1.0",
  "description": "Unified authentication UI types and utilities for multi-provider login",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["auth", "authentication", "wallet", "oauth", "login", "ui", "modal"],
  "license": "MIT"
}
