{
  "name": "@b0ase/flow-canvas",
  "version": "0.1.0",
  "description": "SVG-based workflow canvas with nodes, connections, and drag-drop support",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./canvas": {
      "types": "./dist/canvas.d.ts",
      "import": "./dist/canvas.mjs",
      "require": "./dist/canvas.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "flow",
    "canvas",
    "diagram",
    "workflow",
    "svg",
    "node",
    "edge",
    "graph"
  ],
  "license": "MIT"
}
